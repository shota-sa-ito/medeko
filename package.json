    "@swup/preload-plugin": "^3.2.10",
    "@types/node": "^22.7.5",
    "@vanilla-extract/css": "^1.16.0",
    "astro": "^4.16.2",
    "cropperjs": "^1.6.2",
    "eslint-plugin-perfectionist": "^3.8.0",
